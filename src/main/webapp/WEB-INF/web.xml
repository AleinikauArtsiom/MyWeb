<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <filter>
    <filter-name>MyFirstFilterName</filter-name>
    <filter-class>Filter.MyFirstFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>MyFirstFilterName</filter-name>
    <servlet-name>MyFirstServletName</servlet-name>
  </filter-mapping>
  <listener>
    <listener-class>Listener.MyFirstListenerContexUp</listener-class>
  </listener>
  <listener>
    <listener-class>Listener.ListenerRequestTms</listener-class>
  </listener>


  <servlet>
    <servlet-name>MyFirstServletName</servlet-name>
    <servlet-class>Filter.Servlet.MainFirstServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>MyFirstServletName</servlet-name>
    <url-pattern>/my-first-servlet/*</url-pattern>
  </servlet-mapping>

</web-app>